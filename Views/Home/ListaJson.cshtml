@*Vista que recibe una lista con los datos leidos del fichero json*@

@model List<Nexodo.Models.Json>

@{
    ViewBag.Title = "Lista Json";
}

<h2>ListaJson</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table table-hover">  
    <tr>
        <th>ACTIVE</th>
        <th>ID</th>
        <th>COLOR</th>
        <th>NAME</th>
        <th>COSTPRICE</th>
        <th>PRICE1</th>
        <th>PRICE2</th>
        <th>PRICE3</th>
        <th>PRICE4</th>
        <th>PRICE5</th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>@item.active</td>
    <td>@item.id</td>
    <td>@item.color</td>
    <td>@item.name</td>
    <td>@item.costprice</td>
    <td>@item.price1</td>
    <td>@item.price2</td>
    <td>@item.price3</td>
    <td>@item.price4</td>
    <td>@item.price5</td>
